<%@string colname="colProds"%>
<%@string rowname="rowProds"%>
<%@string fcnname="colRowProds_subset"%>
<%@meta title="${colname}() and ${rowname}() benchmarks on subsetted computation"%>
<%@meta author="Dongcan Jiang"%>
